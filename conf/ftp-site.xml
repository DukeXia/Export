<?xml version="1.0" encoding="UTF-8"?>

<configuration>
	<property>
		<name>export.ftp.type</name>
		<value>SFTP</value>
		<description>FTP类型，可选：FTP、SFTP</description>
	</property>
	<property>
		<name>export.ftp.host</name>
		<value>10.16.0.245</value>
		<description>FTP服务器地址</description>
	</property>
	<property>
		<name>export.ftp.port</name>
		<value>22</value>
		<description>FTP服务器端口，默认情况：FTP(21)、SFTP(22)</description>
	</property>
	<property>
		<name>export.ftp.user</name>
		<value>etl_prd_ups</value>
